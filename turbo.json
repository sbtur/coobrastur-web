{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["API_URL_COOB"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "env": ["API_URL_COOB"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "env": ["API_URL_COOB"]
    },
    "storybook": {
      "cache": false,
      "persistent": true
    }
  }
}
